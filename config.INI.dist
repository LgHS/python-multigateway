[APP]
# Leave empty to disable logging
LOGGING_FILE=multigateway.log
# Notices the admin on the chat that some errors occured on the server
admin_username=

[RC]
HOST=
PORT=
HOOK_ADDR=
AVATAR_URL=
msgtemplate==*{sender}:* {msg}
jointemplate=*{sender} left the channel*
quittemplate=*{sender} joined the channel*

[IRC]
HOST=
PORT=
ROOM=#example
BOT_NAME=
msgtemplate=<RC:{sender}> {msg}

# Can be empty
PASSWORD=

# Goes into ~example@yourprovidername.com when you log in
I=
DESCRIPT=

# Can be empty
welcome_msg=